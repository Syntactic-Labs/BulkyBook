@model IEnumerable<Vendor>
@{
    ViewData["Title"] = "Vendor";
}

<h1>Vendor List</h1>

<table class="table table-bordered table-striped table-hover" style="width 100%">
    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var vendor in Model)
        {
            <tr>
                <td>@vendor.Code</td>
                <td>@vendor.Name</td>
                <td>@vendor.Address</td>
                <td>@vendor.City</td>
                <td>@vendor.State</td>
                <td>@vendor.Zip</td>
                <td>@vendor.Phone</td>
                <td>@vendor.Email</td>
            </tr>
        }
    </tbody>
</table>